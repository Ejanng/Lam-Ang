},
"230": {
&"character": "Don Unnayan",
&"id": "230",
&"next_id": "232",
&"text": "What is your name, young man?",
&"type": "dialogue"
},
"231": {
&"id": "231",
&"next_id": "232",
&"type": ""
},
"232": {
&"character": "Lam-ang",
&"id": "232",
&"next_id": "234",
