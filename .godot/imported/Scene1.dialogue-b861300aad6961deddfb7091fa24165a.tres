},
"165": {
&"id": "165",
&"next_id": "166",
&"type": ""
},
"166": {
&"character": "Lam-ang",
&"id": "166",
&"next_id": "168",
&"text": "Calanutian lies to the east. A new challenge awaits — one I cannot solve with a sword alone.",
&"type": "dialogue"
},
"167": {
&"id": "167",
&"next_id": "168",
&"type": ""
},
"168": {
