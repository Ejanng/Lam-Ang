"120": {
&"character": "Lam-ang",
&"id": "120",
&"next_id": "122",
&"text": "Father, your death is avenged. Nalbuan is safe. No more raids. No more fear.",
&"type": "dialogue"
},
"121": {
&"id": "121",
&"next_id": "122",
&"type": ""
},
"122": {
&"character": "Lam-ang",
&"id": "122",
&"next_id": "124",
&"text": "I wish you could have met the man I've become. I hope... I hope I've made you proud.",
&"type": "dialogue"
},
"123": {
&"id": "123",
&"next_id": "124",
&"type": ""
},
"124": {
&"character": "Lam-ang",
&"id": "124",
&"next_id": "128",
&"text": "I will bring you home. Rest now.",
&"type": "dialogue"
},
"127": {
